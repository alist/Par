<HEAD> range (a b{2,3} c)
<DEF> range (a b{2,3} c) a('^a') b('^b') c('^c')
<IN> a bbb c
<OUT>
0, 1: range :
1, 2:  a : a
2, 2:  b : b
3, 2:  b : b
4, 2:  b : b
5, 2:  c : c

<HEAD> lookahead (a ~bc de)
<DEF> lookahead(a ~bc de) a('^a') bc(b c) de(d e) b('^b') c('^c') d('^d') e('^e')
<IN> a b c d e
<OUT>
0, 1: lookahead :
1, 2:  a : a
2, 2:  ~bc :
3, 3:   b : b
4, 3:   c : c
5, 2:  de :
6, 3:   d : d
7, 3:   e : e
<IN> a b c d
<OUT>
0, 1: lookahead :
1, 2:  a : a

<HEAD> lookahead (a ~b c ~d e)+
<DEF> lookahead(a ~b c ~d e)+ a('^a') c('^c') e('^e')
<IN> a b c d e
<OUT>
0, 1: lookahead :
1, 2:  a : a
2, 2:  ~b : b
3, 2:  c : c
4, 2:  ~d : d
5, 2:  e : e
<IN> a b c d
<OUT>
0, 1: lookahead :
1, 2:  a : a
2, 2:  ~b : b
3, 2:  c : c
<IN> a b c
<OUT>
0, 1: lookahead :
1, 2:  a : a
2, 2:  ~b : b
3, 2:  c : c
<IN> a b
<OUT>
0, 1: lookahead :
1, 2:  a : a
<IN> a
<OUT>
